
<h1> WELCOME TO THE SESSIONS PAGE </h1>

<%= form_for new_user_path do |form| %>
    <p><%= form.label :username %></p>
    <p><%= form.text_field :username %></p>
    <p><%= form.label :email %></p>
    <p><%= form.email_field :email%></p>
    <p><%= form.label :password %></p>
    <p><%= form.password_field :password %></p>
    <p><%= form.label :remember_digest %></p>
    <p><%= form.password_field :remember_digest%></p>
    <p><%= form.submit %></p>
<% end %>

